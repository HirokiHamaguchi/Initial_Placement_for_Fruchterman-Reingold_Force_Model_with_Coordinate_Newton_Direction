{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "g++ build active file",
      "command": "/usr/bin/g++-10",
      "args": [
        "-g",
        "${fileBasename}",
        "-o",
        "${fileBasenameNoExtension}.out",
        "-std=c++17",
        "-Dhari64",
        "-Wall",
        "-Wextra",
        "-Wfatal-errors",
        "-fdiagnostics-color=always",
        // "-D_GLIBCXX_DEBUG",
        "-I/usr/include/eigen3",
        "-I${workspaceFolder}/src/cpp/include",
        "-O2",
        // "-O3",
        "-DDBG_MACRO_NO_WARNING",
        "-I/home/hari64boli64/ac-library-master"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
    }
  ],
  "version": "2.0.0"
}